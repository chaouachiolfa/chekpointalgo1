algorithm recherche
var 
tab1:array_ofinteger[];
tab2:array_ofinteger[];
s:integer:=0;
i,j:integer;
begin
i:=0;
j:=0;
while(i < length.tab1 )do
FOR j FROM 0 TO length.tab2 DO
       Read(tab2[j]);
       read(tab1[i]);
if(tab1[i]=tab2[j] )then
{
    i:=i+1;
}
else if (tab1[i]<>tab2[j]){
    s:=s+tab1[i]
    i:=i+1
}
end if;


end_for
 END_WHILE

write(s)



end
